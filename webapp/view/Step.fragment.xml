<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:u="sap.ui.unified"
    controllerName="zfsaccountcreation.controller.MainWizard"
>
    <!-- <form:SimpleForm
        editable="true"
        layout="ResponsiveGridLayout"
    > -->
        <Label
            text="Step 1"
            required="true"
        />
        <Input
            editable="false"
            value="{StepModel>/SelectedStep1}"
            visible="{StepModel>/Step1Visibility}"
        />

        <Label
            text="Step 2"
            required="true"
        />
        <Input
            editable="false"
            value="{StepModel>/SelectedStep2}"
            visible="{StepModel>/Step2Visibility}"
        />

        <Label
            text="Step 3"
            required="true"
        />
        <Input
            editable="false"
            value="{StepModel>/SelectedStep3}"
            visible="{StepModel>/Step3Visibility}"
        />
        <Label
            text="Step 4"
            required="true"
        />
        <Input
            editable="false"
            value="{StepModel>/SelectedStep4}"
            visible="{StepModel>/Step4Visibility}"
        />

       
    <!-- </form:SimpleForm> -->
</core:FragmentDefinition>
